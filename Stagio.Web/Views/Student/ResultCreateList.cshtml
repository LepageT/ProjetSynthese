@model IEnumerable<Stagio.Web.ViewModels.Student.ListStudent>

@{
    ViewBag.Title = "ResultCreateList";
}

<h2 id ="resultCreateList-page">Les étudiants suivants n'ont pas pu être ajouté car ils sont déjà inscrits</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Matricule)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Matricule)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
    </tr>
}

</table>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <div class="panel-body">

        <input id="resultCreateList-button" type="submit" value="Ok" />

    </div>
}



<div>
    @Html.ActionLink("Retour à l'accueil", MVC.Home.Index())
</div>