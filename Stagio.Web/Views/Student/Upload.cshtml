@using System.Web.Mvc

<h2 id="upload-page">Importer les matricules des étudiants</h2>

<p>Le fichier doit être un fichier csv</p>

<form action="" method="post" enctype="multipart/form-data">

    <label for="file">Fichier csv:</label>
    <input  type="file" name="file" id="file" />
 
    <input type="submit" id="button-upload"/>
       
</form>

<div>
    @ViewBag.Message
</div>

<div>
    @Html.ActionLink("Retour à l'accueil", MVC.Home.Index())
</div>
