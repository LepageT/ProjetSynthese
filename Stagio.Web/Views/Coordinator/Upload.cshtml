@using Stagio.Web.Module.Strings.Shared;
<h2 id="upload-page">Importer les matricules des étudiants</h2>

<p>Le fichier doit être un fichier csv</p>

<form action="" method="post" enctype="multipart/form-data">

    <div><label for="file">Fichier csv:</label></div>
    <div><input  type="file" name="file" id="file" /><br/></div>
 
    <div><input type="submit" id="button-upload" value="Importer" class="btnMenu btnMenu-default" style="width:100px" /></div>
       
</form>

<div>
    @ViewBag.Message
</div>
<br />
<div>
    @Html.ActionLink(Navigation.ReturnHome, MVC.Home.Index())
</div>
